{
	"parser": "babel-eslint",
    "extends": "airbnb",
    "rules": {
        "indent": [1, "tab"],
		"no-tabs": "off",
		"no-multi-spaces": "off",
		"key-spacing": "off",
		"no-plusplus": "off",
		"no-use-before-define": "off",
		"react/jsx-indent": [2, "tab"],
		"react/jsx-indent-props": [2, "tab"]
    }
}